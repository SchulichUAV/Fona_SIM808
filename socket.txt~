AT+CSTT="internet.fido.ca","fido","fido"
#AT+SAPBR=3,1,"USER","fido"
#AT+SAPBR=3,1,"PWD","fido"
#Start connection
#AT+SAPBR=1,1
#expect:OK
#Check connection and get current IP
AT+CIICR
wait:10
AT+CIFSR
#Start HTTP service
AT+CIPSTART="TCP","136.159.49.115","80"
expect:CONNECT OK
AT+CIPSEND
data:TEST MESSAGE
AT+CIPSHUT
expect:SHUT OK
